<h1>欢迎注册</h1>
<form action="/Register/SaveRegisterData" method="post">
    <label>角色</label>
    <select name="Role">
        <option value="0">维修骑士</option>
        <option value="1">用户</option>
    </select><br />
    <label>姓名</label>
    <input type="text" name="Name" /><br />
    <label>性别</label>
    <input type="radio" name="Gender" value="男" />男
    <input type="radio" name="Gender" value="女" />女<br />
    <label>学号或工号</label>
    <input type="number" name="RuijieId" /><br />
    <label>密码</label>
    <input type="password" name="Password" /><br />
    <label>确认密码</label>
    <input type="password" name="ConfirmPassword" /><br />
    <label>移动电话</label>
    <input type="tel" name="Telphone" /><br />
    <label>楼宇和房间号</label>
    <input type="text" name="Dormitory" value="B座218" /><br />
    <label>Mac地址</label>
    <input type="text" name="MacAddress" /><br />
    <input type="submit" value="注册" /><br>
</form>